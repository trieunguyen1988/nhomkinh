


# BEGIN WordPress
<IfModule mod_rewrite.c>
RewriteEngine On
RewriteBase /freelance/nhomkinh/
RewriteRule ^index\.php$ - [L]
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule . /freelance/nhomkinh/index.php [L]
</IfModule>

# END WordPress
<Files wp-config.php>;
    order allow,deny
    deny from all
</Files>